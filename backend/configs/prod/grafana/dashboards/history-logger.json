{
  "title": "History Logger Service",
  "tags": [
    "history-logger",
    "telemetry",
    "monitoring"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "15s",
  "panels": [
    {
      "id": 1,
      "title": "Service Status",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "up{job=\"history-logger\"}",
          "refId": "A",
          "legendFormat": "History Logger"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "DOWN",
                  "color": "red"
                },
                "1": {
                  "text": "UP",
                  "color": "green"
                }
              }
            }
          ]
        }
      }
    },
    {
      "id": 2,
      "title": "Queue Size",
      "type": "gauge",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "telemetry_queue_size",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 5000,
                "color": "yellow"
              },
              {
                "value": 10000,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Queue Age (seconds)",
      "type": "gauge",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "telemetry_queue_age_seconds",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 60,
                "color": "yellow"
              },
              {
                "value": 300,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Processing Rate (msg/sec)",
      "type": "gauge",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "rate(telemetry_processed_total[1m])",
          "refId": "A"
        }
      ]
    },
    {
      "id": 5,
      "title": "Telemetry Flow",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "rate(telemetry_received_total[1m])",
          "refId": "A",
          "legendFormat": "Received"
        },
        {
          "expr": "rate(telemetry_processed_total[1m])",
          "refId": "B",
          "legendFormat": "Processed"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "id": 6,
      "title": "Batch Size Distribution",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(telemetry_batch_size_bucket[5m]))",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(telemetry_batch_size_bucket[5m]))",
          "refId": "B",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(telemetry_batch_size_bucket[5m]))",
          "refId": "C",
          "legendFormat": "p99"
        }
      ]
    },
    {
      "id": 7,
      "title": "Processing Duration",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(telemetry_processing_duration_seconds_bucket[5m]))",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(telemetry_processing_duration_seconds_bucket[5m]))",
          "refId": "B",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(telemetry_processing_duration_seconds_bucket[5m]))",
          "refId": "C",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 8,
      "title": "Redis Operations Duration",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(redis_operation_duration_seconds_bucket[5m]))",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(redis_operation_duration_seconds_bucket[5m]))",
          "refId": "B",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 9,
      "title": "Dropped Messages",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "targets": [
        {
          "expr": "rate(telemetry_dropped_total[5m])",
          "refId": "A",
          "legendFormat": "{{reason}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      }
    },
    {
      "id": 10,
      "title": "Database Errors",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 20
      },
      "targets": [
        {
          "expr": "rate(database_errors_total[5m])",
          "refId": "A",
          "legendFormat": "{{error_type}}"
        }
      ]
    },
    {
      "id": 11,
      "title": "Node Registration",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 28
      },
      "targets": [
        {
          "expr": "rate(node_hello_received_total[5m])",
          "refId": "A",
          "legendFormat": "Received"
        },
        {
          "expr": "rate(node_hello_registered_total[5m])",
          "refId": "B",
          "legendFormat": "Registered"
        },
        {
          "expr": "rate(node_hello_errors_total[5m])",
          "refId": "C",
          "legendFormat": "Errors"
        }
      ]
    },
    {
      "id": 12,
      "title": "Node Hello Errors by Type",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 28
      },
      "targets": [
        {
          "expr": "rate(node_hello_errors_total[5m])",
          "refId": "A",
          "legendFormat": "{{error_type}}"
        }
      ]
    },
    {
      "id": 13,
      "title": "Heartbeat Messages",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 36
      },
      "targets": [
        {
          "expr": "rate(heartbeat_received_total[5m])",
          "refId": "A",
          "legendFormat": "{{node_uid}}"
        }
      ]
    },
    {
      "id": 14,
      "title": "Laravel API Duration",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 36
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(laravel_api_request_duration_seconds_bucket[5m]))",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(laravel_api_request_duration_seconds_bucket[5m]))",
          "refId": "B",
          "legendFormat": "p95"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        }
      }
    },
    {
      "id": 15,
      "title": "Unknown Node Events Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 44
      },
      "targets": [
        {
          "expr": "rate(node_event_unknown_total[5m])",
          "refId": "A",
          "legendFormat": "unknown event rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 0.05,
                "color": "red"
              }
            ]
          }
        }
      }
    }
  ]
}
