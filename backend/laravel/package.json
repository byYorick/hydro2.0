{
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "typecheck": "vue-tsc --noEmit -p tsconfig.json",
        "lint": "eslint . --ext .ts,.tsx,.vue",
        "test": "vitest run",
        "test:ui": "vitest --ui",
        "e2e": "playwright test",
        "e2e:ci": "npm run build && playwright install --with-deps && playwright test",
        "e2e:browser": "playwright test --config=../../tests/e2e/browser/playwright.config.ts",
        "e2e:browser:ui": "playwright test --ui --config=../../tests/e2e/browser/playwright.config.ts"
    },
    "devDependencies": {
        "@inertiajs/vue3": "^2.0.0",
        "@playwright/test": "^1.56.1",
        "@tailwindcss/forms": "^0.5.3",
        "@testing-library/jest-dom": "^6.9.1",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-vue": "^5.0.0",
        "@vitejs/plugin-vue-jsx": "^5.1.1",
        "@vitest/ui": "^4.0.6",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/compiler-dom": "^3.4.0",
        "@vue/server-renderer": "^3.5.24",
        "@vue/test-utils": "^2.4.6",
        "autoprefixer": "^10.4.12",
        "axios": "^1.6.4",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.23.0",
        "jsdom": "^27.2.0",
        "laravel-vite-plugin": "^1.0",
        "postcss": "^8.4.31",
        "tailwindcss": "^3.2.1",
        "typescript": "^5.3.3",
        "vite": "^5.0",
        "vitest": "^4.0.6",
        "vue": "^3.4.0",
        "vue-tsc": "^2.0.0"
    },
    "dependencies": {
        "echarts": "^6.0.0",
        "laravel-echo": "^2.1.1",
        "pinia": "^2.1.7",
        "pusher-js": "^8.3.0",
        "vue-echarts": "^8.0.1",
        "vue-virtual-scroller": "^2.0.0-beta.8"
    }
}
